{
  "name": "teched2022-dt181",
  "version": "1.0.0",
  "description": "CAP flight demo scenario",
  "repository": "https://github.com/SAP-samples/teched2022-DT181",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@sap/cds": "^6.8.1",
    "@sap/cds-dk": "^6.8.1",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/xssec": "^3.2.7",
    "@sapui5/ts-types": "^1.92.2",
    "eslint": "^7.32.0",
    "express": "^4.18.2",
    "hdb": "^0.19.1",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "@sap/ux-specification": "UI5-1.129",
    "axios": "^1.4.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jest": "^29.0.2",
    "qunit": "^2.9.3",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds run",
    "test": "jest --runInBand",
    "local-watch": "PORT=4005 node './node_modules/@sap/cds-dk/bin/cds.js' watch --open managetravels/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-managetravels": "cds watch --open managetravels/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "[production]": {
        "db": {
          "kind": "hana-cloud"
        },
        "auth": {
          "kind": "xsuaa"
        }
      },
      "auth": {
        "[development]": {
          "strategy": "dummy"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "log": {
      "service": true
    },
    "features": {
      "preserve_computed": true
    },
    "build": {
      "[node]": {
        "target": "gen",
        "tasks": [
          {
            "for": "node-cf"
          },
          {
            "for": "hana"
          }
        ]
      },
      "[hana]": {
        "target": "gen",
        "tasks": [
          {
            "for": "hana"
          }
        ]
      }
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 20000
  },
  "license": "SAP SAMPLE CODE LICENSE",
  "private": true,
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "sapux": [
    "app/managetravels"
  ]
}
